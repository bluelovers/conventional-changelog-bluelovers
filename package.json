{
  "name": "@bluelovers/conventional-changelog-bluelovers",
  "version": "5.0.19",
  "description": "mixin angular/emoji preset of conventional-changelog",
  "keywords": [
    "conventional-changelog",
    "angular",
    "emoji",
    "preset"
  ],
  "homepage": "https://github.com/bluelovers/conventional-changelog-bluelovers#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/conventional-changelog-bluelovers/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/conventional-changelog-bluelovers.git"
  },
  "license": "ISC",
  "author": "Steve Mao",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "conventional-changelog.js",
    "conventional-recommended-bump.js",
    "index.js",
    "parser-opts.js",
    "writer-opts.js",
    "lib",
    "templates"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "test-windows": "mocha --timeout 30000 ./test/test.js",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --bump patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "prepublishOnly_": "yarn run prepublishOnly:check-bin && yarn run prepublishOnly:update && yarn run test",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort"
  },
  "dependencies": {
    "compare-func": "^2.0.0",
    "emoji-regex": "^9.0.0",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/chai": "^4.2.11",
    "@types/conventional-changelog": "^3.1.0",
    "@types/conventional-changelog-core": "^4.1.1",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/shelljs": "^0.8.8",
    "@types/through2": "^2.0.36",
    "better-than-before": "^1.0.0",
    "chai": "^4.2.0",
    "commit-emojis": "^1.0.0",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-core": "^4.1.7",
    "conventional-changelog-metahub": "^4.0.1",
    "fs-extra": "^9.0.1",
    "git-dummy-commit": "^1.3.0",
    "mocha": "^8.0.1",
    "shelljs": "^0.8.4",
    "through2": "^3.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  }
}
